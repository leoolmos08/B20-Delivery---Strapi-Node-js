{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "phone": {
      "required": true,
      "type": "integer"
    },
    "product": {
      "type": "json",
      "required": true
    },
    "totalPayment": {
      "type": "decimal",
      "required": true
    }
  }
}
